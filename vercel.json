{
    "version": 2,
    "builds": [
      {
        "src": "PWA/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "REST/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "REST/push-notification.js",
        "use": "@vercel/node"
      },
      {
        "src": "REST/shopping-list.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/server", "dest": "REST/server.js" },
      { "src": "/api/push-notification", "dest": "REST/push-notification.js" },
      { "src": "/api/shopping-list", "dest": "REST/shopping-list.js" },
      { "src": "/(.*)", "dest": "PWA/$1" }
    ]
  }  